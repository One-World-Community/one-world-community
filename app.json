{
  "expo": {
    "name": "one-world-community",
    "slug": "one-world-community",
    "owner": "one-world-community",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "oneworldcommunity",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.oneworldcommunity.oneworldcommunity",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.oneworldcommunity.oneworldcommunity"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "baseUrl": "/one-world-community"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "88f3d8bc-d3a5-44ec-839f-330f9be4c495"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/88f3d8bc-d3a5-44ec-839f-330f9be4c495"
    },
    "newArchEnabled": true
  }
}
